package src.main.java.com.qa.java.Project_1;

public class Runner {

	public static void main(String args[]) {
		QAC ref1, ref2, ref3;
		ref1 = QAC.createObject();
		ref2 = QAC.createObject();
		ref3 = QAC.createObject();
		System.out.println(QAC.pound);
		System.out.println(QAC.dollar);
		ref1.setName("RBS");
		ref2.setName("HSBC");
		ref3.setName("Barclays");
		
		System.out.println("Name: "+ref1.getName());
		System.out.println("Name: "+ref2.getName());
		System.out.println("Name: "+ref3.getName());
	}
}