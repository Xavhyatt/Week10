package src.main.java.com.qa.java.Project_1;

import java.awt.BorderLayout;
import java.awt.TextField;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

public class EventClass implements ActionListener {
	TextField textField

	public void actionPerformed(ActionEvent e) {
		System.out.println("actionPerformed Method");
		FormGUI.counter++;
		TextField textField = new TextField();
		textField.setText("Click"+count);
		FormGUI.frame1.add(textField, BorderLayout.CENTER);
		//FormGUI.getFrame().add(textField, BorderLayout.CENTER);
		FormGUI.frame1.setVisible(true);
		
	}
	public EventClass(TextField txt) {
		textField = txt;
	}

}
