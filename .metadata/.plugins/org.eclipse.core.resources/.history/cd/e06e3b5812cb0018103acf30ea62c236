package src.main.java.com.qa.java.Project_1;

import java.awt.BorderLayout;
import java.awt.Frame;
import java.awt.TextField;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

public class EventClass implements ActionListener {
	public void actionPerformed(ActionEvent e) {
		TextField textField = new TextField();
		textField.setText("BLUE");
		//System.out.println("actionPerformed Method");
		
	}
	public void actionPerformed(ActionEvent e, Frame frame) {
		TextField textField = new TextField();
		
		textField.setText("BLUE");
		frame.add(textField, BorderLayout.CENTER);
		//System.out.println("actionPerformed Method");
		
	}

}
